{
  "name": "cloudforge-ai-frontend",
  "version": "1.0.0",
  "description": "CloudForge AI Frontend - Next.js Dashboard",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_DISABLE_SWC_NATIVE=1 next dev",
    "build": "cross-env NEXT_DISABLE_SWC_NATIVE=1 next build",
    "start": "cross-env NEXT_DISABLE_SWC_NATIVE=1 next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "cypress run --spec 'cypress/e2e/**/*.cy.{js,jsx,ts,tsx}'",
    "e2e:headless": "cypress run --headless",
    "analyze": "cross-env ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.7",
    "lucide-react": "^0.451.0",
    "monaco-editor": "^0.52.0",
    "next": "15.5.4",
    "postcss": "^8.4.47",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-dropzone": "^14.2.9",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.13.1",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8",
    "recharts": "^2.12.7",
    "remark-gfm": "^4.0.0",
    "swr": "^2.2.5",
    "tailwindcss": "^3.4.14",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "15.5.4",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/nextjs": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.7.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-virtualized": "^9.21.30",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "cross-env": "^7.0.3",
    "cypress": "15.3.0",
    "eslint": "^9.12.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "storybook": "^8.3.5",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@/components/(.*)$": "<rootDir>/src/components/$1",
      "^@/lib/(.*)$": "<rootDir>/src/lib/$1",
      "^@/styles/(.*)$": "<rootDir>/src/styles/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "keywords": [
    "cloudforge",
    "ai",
    "cloud-management",
    "nextjs",
    "react",
    "typescript",
    "kubernetes",
    "microservices",
    "dashboard"
  ],
  "author": "CloudForge AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudforge-ai/cloudforge-ai.git"
  },
  "bugs": {
    "url": "https://github.com/cloudforge-ai/cloudforge-ai/issues"
  },
  "homepage": "https://cloudforge-ai.com"
}
